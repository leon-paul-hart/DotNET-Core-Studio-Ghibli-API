    "iisExpress": {
      "applicationUrl": "http://localhost:17194",
      "sslPort": 44314
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "StudioGhibliApi": {
      "commandName": "Project",
